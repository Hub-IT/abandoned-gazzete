<!DOCTYPE html>
<html class="no-js">
<head>
    {% block head %}
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
        <title>{% block title %} Default Title {% endblock %} | Gazzete</title>
        <meta name="description" content="">
        <meta name="viewport" content="width=device-width">

        {{ html.link("bower_components/bootstrap/dist/css/bootstrap.min.css")|raw }}
        {{ html.link("bower_components/fontawesome/css/font-awesome.min.css")|raw }}
        {{ html.link("css/main.css")|raw }}

        <!--[if lt IE 9]>
        {{ html.script("bower_components/html5shiv/dist/html5shiv.min.js")|raw }}
        <![endif]-->

        {{ html.script("js/modernizr.custom.32229-2.8-respondjs-1-4-2.js")|raw }}
    {% endblock head %}
</head>
<body>
<header class="hero-image" role="banner"
        style="background-image: url(img/default-about.jpg);">
    {#{% block hero %}style="background-image: url({{ html.url('img/default-about.jpg') }} {% endblock hero %})">#}

    <span class="menu-trigger animated fadeInDown">
        <span class="bar"></span>
        <span class="bar"></span>
        <span class="bar"></span>
    </span>

    {% include 'consumer/layouts/_partials/nav.twig' %}

</header>

<main class="container">
    <div class="row">
        <div class="col-xs-12 single-content">
            <!-- contents per page -->
            {% block content %}{% endblock content %}

            {% include 'consumer/layouts/_partials/footer.twig' %}
        </div>
    </div>
</main>
<!-- /container -->

<script src="//ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js"></script>
<script>window.jQuery || {{ html.documentWriteScript("bower_components/jquery/dist/jquery.min.js")|raw }}</script>

{{ html.script("bower_components/jPanelMenu/jquery.jpanelmenu-legacy.js")|raw }}
{{ html.script("bower_components/bootstrap/dist/js/bootstrap.min.js")|raw }}
{{ html.script("bower_components/fastclick/lib/fastclick.js")|raw }}

{{ html.script("js/main.js")|raw }}

</body>
</html>
